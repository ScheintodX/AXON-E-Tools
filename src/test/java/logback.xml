<configuration>
	<appender name="ConsoleOut" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-5p: %c{1} %m%n</pattern>
		</encoder>
	</appender>
	<logger name="de.axone" level="INFO"/>
	<root level="INFO">
		<appender-ref ref="ConsoleOut"/>
	</root>
</configuration>
